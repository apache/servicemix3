<?xml version="1.0" encoding="UTF-8"?>
<beans version="1.0" xmlns:foo="http://www.foo.com/ns/bar">
  <bean class="org.apache.servicemix.jbi.deployment.Jbi" id="jbi">

    <property name="component">
      <bean class="org.apache.servicemix.jbi.deployment.Component">
        <property name="type" value="service-engine"/>


        <foo:Configuration version="1.0">
          <foo:ThreadPool size="10"/>
          <foo:Queue1 size="50"/>
        </foo:Configuration>
        <property name="sharedLibraries">
          <list>
            <bean class="org.apache.servicemix.jbi.deployment.SharedLibrary">slib1</bean>
          </list>
        </property>
        <property name="identification">
          <bean class="org.apache.servicemix.jbi.deployment.Identification">


            <foo:TypeInfo part-number="012AB490-578F-114FAA">
        BPEL:2.0:XQuery:1.0:XPath:2.0:XPath:1.0
            </foo:TypeInfo>
            <property name="name" value="example-engine-1"/>
            <property name="description" value="An example service engine"/>
          </bean>
        </property>
        <property name="componentClassName" value="com.foo.Engine1"/>
        <property name="description" value="foo"/>
        <property name="componentClassPath">
          <bean class="org.apache.servicemix.jbi.deployment.ClassPath">

            <property name="pathElements">
              <list>
                <value>Engine1.jar</value>
              </list>
            </property>
          </bean>
        </property>
        <property name="bootstrapClassName" value="com.foo.Engine1Bootstrap"/>
        <property name="bootstrapClassPath">
          <bean class="org.apache.servicemix.jbi.deployment.ClassPath">

            <property name="pathElements">
              <list>
                <value>Engine1.jar</value>
              </list>
            </property>
          </bean>
        </property>
      </bean>
    </property>
  </bean>
</beans>